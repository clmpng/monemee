# Mission Control Backend Configuration

# Server
PORT=5001
NODE_ENV=development

# Database (verbindet sich zur Monemee PostgreSQL DB)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=monemee
DB_USER=postgres
DB_PASSWORD=

# Admin Auth
ADMIN_JWT_SECRET=your-super-secret-admin-jwt-key-change-this
ADMIN_SESSION_DURATION=24h

# CORS (Frontend URL)
FRONTEND_URL=http://localhost:5173

# WebSocket
WS_PORT=5002

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100
